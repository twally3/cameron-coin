{
  "name": "cameron",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --no-deprecation --no-warnings --experimental-modules index.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "linebreak-style": 0,
      "no-console": 0,
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    },
    "globals": {}
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "ws": "^6.1.0"
  }
}
